{
  "name": "@chiaweilee/filters",
  "version": "0.0.1",
  "private": false,
  "main": "lib/install.js",
  "module": "src/install.js",
  "scripts": {
    "build": "node scripts/build-index.js",
    "lint": "eslint --ext .js,.vue,.json src --fix",
    "test": "mocha",
    "es5": "npx babel --no-babelrc src --out-dir lib",
    "commit": "npx git-cz"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^5.10.0",
    "lint-staged": "^8.1.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verify-commit-msg.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  }
}
